  "categories": ["business", "productivity", "utilities"],
  "screenshots": [
    {
      "src": "/screenshots/dashboard.png",
      "sizes": "1280x720",
      "type": "image/png"
    },
    {
      "src": "/screenshots/assessment.png",
      "sizes": "1280x720",
      "type": "image/png"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View your organization dashboard",
      "url": "/dashboard",
      "icons": [{ "src": "/icons/dashboard-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Assessment",
      "short_name": "Assessment",
      "description": "Continue your assessment",
      "url": "/assessment",
      "icons": [{ "src": "/icons/assessment-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Compliance",
      "short_name": "Compliance",
      "description": "Check compliance status",
      "url": "/compliance",
      "icons": [{ "src": "/icons/compliance-96x96.png", "sizes": "96x96" }]
    }
  ],
  "share_target": {
    "action": "/share-document",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "documents",
          "accept": ["application/pdf", "image/*", ".doc", ".docx"]
        }
      ]
    }
  }
