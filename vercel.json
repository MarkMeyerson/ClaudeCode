{
  "installCommand": "cd frontend && npm install",
  "buildCommand": "cd frontend && npx vite build",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/assessment/questions",
      "destination": "/api/get-questions"
    },
    {
      "source": "/api/assessment/start",
      "destination": "/api/save-lead"
    },
    {
      "source": "/api/assessment/:id/submit",
      "destination": "/api/submit-assessment"
    },
    {
      "source": "/api/assessment/:id/results",
      "destination": "/api/get-results"
    }
  ]
}
