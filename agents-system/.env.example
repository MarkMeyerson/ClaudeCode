# Environment Configuration
NODE_ENV=development
LOG_LEVEL=info

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ai_assessment
DB_USER=postgres
DB_PASSWORD=your_database_password_here

# Stripe API Configuration (Required for Finance Tracker)
STRIPE_API_KEY=sk_test_your_stripe_api_key_here
# Use sk_test_ for testing, sk_live_ for production

# Claude/Anthropic API Configuration (Optional - for AI features)
CLAUDE_API_KEY=sk-ant-your_claude_api_key_here
# or
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key_here

# Finance Tracker Configuration (Optional - Override defaults)
FINANCE_MONTHLY_REVENUE_GOAL=50000
FINANCE_ANNUAL_REVENUE_GOAL=600000
FINANCE_FEDERAL_TAX_RATE=0.21
FINANCE_STATE_TAX_RATE=0.05

# Other Agent Configurations (for future agents)
# CONTENT_CREATOR_API_KEY=...
# LEAD_QUALIFIER_API_KEY=...
